{
    "title": "wsj0051的个人笔记",
    "author": "wsj0051",
    "root": ".",
    "description": "个人笔记",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "links": {
        "sidebar": {
            "导航": "/home"
        }
    },
    "styles": {
        "website": "styles/style.css"
    },
    "structure": {
        "readme": "home.md"
    },
    "variables": {
        "wsj0051": "wsj0051"
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-pro",
        "-sharing",
        "-livereload",
        "edit-link@^2.0.2",
        "my-toolbar",
        "hide-element",
        "donate",
        "theme-comscore",
        "chart@^0.2.0",
        "popup",
        "code",
        "image-captions",
        "flexible-alerts",
        "ace@^0.3.2",
        "favicon-absolute",
        "splitter",
        "expandable-chapters",
        "katex",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "anchor-navigation-ex": {
            "showLevel": false,
            "associatedWithSummary": true,
            "printLog": false,
            "multipleH1": true,
            "mode": "float",
            "showGoTop": true,
            "float": {
                "floatIcon": "fa fa-navicon",
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "chart": {
            "type": "c3"
        },
        "my-toolbar": {
            "buttons": [
                {
                    "label": "",
                    "icon": "fa fa-file-pdf-o",
                    "url": "/book.pdf",
                    "position": "left",
                    "text": "下载PDF",
                    "target": "_blank"
                },
                {
                    "label": "",
                    "icon": "fa fa-home",
                    "url": "/home",
                    "position": "right",
                    "text": "导航",
                    "target": "_blank"
                }
            ]
        },
        "donate": {
            "wechat": "/assets/images/wechat_pay.jpg",
            "button": "打赏",
            "wechatText": "微信扫码"
        },
        "edit-link": {
            "base": "https://github.com/wsj0051/wsj0051.github.io/edit/master",
            "label": "Edit This Page"
        },
        "image-captions": {
            "caption": "图 - _CAPTION_"
        },
        "favicon-absolute": {
            "favicon": "/assets/images/favicon.ico",
            "appleTouchIconPrecomposed152": "/assets/images/apple-touch-icon-152.png"
        },
        "hide-element": {
            "elements": [
                ".gitbook-link"
            ]
        }
    }
}